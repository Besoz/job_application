<div class="page-header">
  <%= link_to interviewers_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_interviewer_path(@interviewer), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show interviewer</h1>
</div>

<dl class="dl-horizontal">
  <dt>Name:</dt>
  <dd><%= @interviewer.name %></dd>

  <dt>Email:</dt>
  <dd><%= @interviewer.email %></dd>

  <dt>Position:</dt>
  <dd><%= @interviewer.position %></dd>

  <dt>Encrypted password:</dt>
  <dd><%= @interviewer.encrypted_password %></dd>

</dl>
