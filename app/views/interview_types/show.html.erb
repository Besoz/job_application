<div class="page-header">
  <%= link_to interview_types_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_interview_type_path(@interview_type), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show interview_type</h1>
</div>

<dl class="dl-horizontal">
  <dt>Name:</dt>
  <dd><%= @interview_type.name %></dd>

  <dt>Description:</dt>
  <dd><%= @interview_type.description %></dd>

  <dt>Duration:</dt>
  <dd><%= @interview_type.duration %></dd>

</dl>
